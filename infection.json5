{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.29.0/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "DependencyInjection",
            "Command",
            "MakrazVerifyEmailChangeBundle.php"
        ]
    },
    "logs": {
        "text": "infection.log",
        "summary": "infection-summary.log"
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 70,
    "minCoveredMsi": 80,
    "phpUnit": {
        "configDir": "."
    },
    "timeout": 30
}
